<div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
  <%= form_tag :session, class: "form-inline" do %>
  <div class="nav navbar-nav navbar-right">
    <div class="form-group ">
      <label for="InputName">ユーザー名</label>
      <%= text_field_tag "name", "", class: "form-control input-sm", id: "InputName"%>
    </div>
    <div class="form-group">
      <label for="InputPassword">パスワード</label>
      <%= password_field_tag "password", "", class: "form-control input-sm", id: "InputPassword" %>
      <%= submit_tag "ログイン", class: "btn btn-default btn-sm", data: { disable_with: "ログイン中" } %>
  <% end %>
</div>
