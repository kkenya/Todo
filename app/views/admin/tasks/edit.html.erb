<div class="page-header">
  <h1>タスクの編集</h1>
</div>

<p><%= link_to "タスクの一覧に戻る", [:admin, @user, :tasks] %></p>

<%= form_for [:admin, @user,@task], role: 'form' do |form| %>
  <%= render "form", form: form %>
  <%= form.submit '更新', class: 'btn btn-default', data: { disable_with: '更新中' } %>
<% end %>